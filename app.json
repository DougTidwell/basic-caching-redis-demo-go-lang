{
  "name": "Redis Cache Example",
  "description": "Fetching number of github repos using Golang, gin and Redis",
  "stack": "container",
  "repository": "https://github.com/redis-developer/basic-caching-redis-demo-go-lang",
  "logo": "https://redis.io/images/redis-white.png",
  "keywords": ["golang", "gin", "redis"],
  "env": {
    "REDIS_HOST": {
      "description": "Redis server host",
      "required": true
    },
    "REDIS_PORT": {
      "description": "Redis server PORT",
      "required": true
    },
    "REDIS_PASSWORD": {
      "description": "Redis server password",
      "required": true
    },
    "API_PUBLIC_PATH": {
      "description": "Public path to frontend, example `/api/public`"
    },
    "API_PORT": {
      "description": "Api public port, example `5000`"
    }
  }
}